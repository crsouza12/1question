<html>
	<head>
		<title>Consegue?</title>
		</head>
		<body>
			/* Try tapping the ball /*
			let ball = svg.append('circle').attr('fill','red'). attr('r',35). attr('cx',150).attr('cy',35);
			ball.on('click',( )=>{
				ball.interrupt().attr('cy',35).attr('cx',150);
	                ball.transition().attr('cy',165).duration(2000);
	          });
</body>
</html>
